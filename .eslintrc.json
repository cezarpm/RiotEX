{
  "env": {
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "babelOptions": {
      "configFile": "./.babelrc"
    },
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "off",
    "import/no-cycle": "off",
    "import/extensions": "off"
  },
  "parser": "babel-eslint"
}
